{
  "name": "enot-simon-home",
  "private": true,
  "description": "enot simon home project",
  "main": "index.js",
  "repository": "https://github.com/enotsimon/home",
  "engines": {
    "node": ">=8.10"
  },
  "scripts": {
    "clean": "rm -rf dist/*",
    "flow": "flow",
    "lint:staged": "lint-staged",
    "lint": "yarn run lint:js && yarn run lint:css",
    "lint:js": "eslint --ext .js --ext .jsx .",
    "lint:css": "stylelint --fix --di \"**/*.scss\"",
    "test": "NODE_PATH=./app mocha --require babel-core/register test/*.test.js test/samples/*.test.js",
    "pre-commit": "lint-staged",
    "serve": "webpack-dev-server --env.serve --mode=development",
    "build": "yarn build-dev",
    "build-dev": "webpack --config webpack.config.js --mode=development",
    "build-test": "webpack --config webpack.config.js --env.test --mode=development",
    "build-prod": "webpack --config webpack.config.js --env.production --mode=production"
  },
  "dependencies": {
    "bootstrap": "3.4.1",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.0.0",
    "d3": "^4.10.0",
    "deep-equal-in-any-order": "^1.0.14",
    "document-promises": "^3.1.3",
    "enot-simon-utils": "https://github.com/enotsimon/utils",
    "file-loader": "^4.2.0",
    "html-webpack-tags-plugin": "^2.0.13",
    "js-yaml": "^3.12.1",
    "pixi.js": "^4.5.3",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "prettyjson": "^1.2.1",
    "ramda": "^0.27.0",
    "random": "^2.1.1",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-router-dom": "^5.1.2",
    "style-loader": "^0.23.1",
    "url-loader": "^2.2.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-decorators": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/plugin-transform-flow-strip-types": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-flow": "^7.10.1",
    "@babel/preset-react": "7.10.1",
    "@babel/register": "^7.10.1",
    "babel-core": "6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-jsx-control-statements": "^4.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-eslatest-node6": "^1.0.1",
    "babel-preset-react-optimize": "^1.0.1",
    "chai": "^4.1.2",
    "chai-string": "^1.4.0",
    "eslint": "5.13.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-import-resolver-babel-module": "5.1.2",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-jsx-control-statements": "^2.2.0",
    "eslint-plugin-react": "7.20.0",
    "flow-bin": "^0.142.0",
    "html-webpack-plugin": "^3.2.0",
    "ignore-styles": "^5.0.1",
    "jsx-control-statements": "^3.2.8",
    "lint-staged": "^6.0.0",
    "loader-utils": "^1.1.0",
    "mocha": "^5.0.0",
    "pre-commit": "^1.2.2",
    "react-docgen": "^2.20.1",
    "react-hot-loader": "^4.2.0",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1"
  },
  "pre-commit": [
    "lint:staged",
    "flow",
    "test"
  ],
  "lint-staged": {
    "*.{cmd,html,json,md,sh,txt,xml,yml}": [
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --di",
      "git add"
    ]
  }
}
