  "pre-commit": [
    "lint:staged"
  ],
  "lint-staged": {
    "*.{cmd,html,json,md,sh,txt,xml,yml}": [
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --di",
      "git add"
    ]
  }
